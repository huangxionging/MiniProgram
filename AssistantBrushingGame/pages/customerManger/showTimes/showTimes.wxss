/* page/brushInfo/showTimes.wxss */
.top_box{background: #ffffff;padding: 20rpx;}
.top_box .t{line-height: 50rpx;text-align: center;color: #9a9a9a;font-size: 26rpx;}
.top_box .m{display: flex;width: 100%;font-size: 24rpx;color: #9a9a9a;text-align: center;}
.top_box .m .l{width: 33.3%;border-right: solid #f1f1f1 1px;position: relative;height: 100rpx;margin-top: 40rpx;}
.canvars_box{position: relative;text-align: left;margin-top: -20px;}
.top_box .m .l .out{width:140px;height: 140px;margin: 0;padding: 0;display: block;}
.top_box .m .l .in{width:140px;height: 140px;margin: 0;padding: 0;display: block;position: absolute;top: 0px;left: 0px;}
.top_box .m .l .mm{position: absolute;left: 0px;top: 6px;width: 118px;text-align: center;display: block;}
.top_box .m .l .mm .mm_t{font-size: 25px;color: #333333;display: block;width: 100%;}
.top_box .m .m{width: 33.3%;border-right: solid #f1f1f1 1px;display: block;margin-top: 20px;height: 50px;}
.top_box .m .m .m_t{font-size: 15px;color: #333333;display: block;width: 100%;}
.spl{text-align: center;color: #444444;}
.top_box .m .r{width: 33.3%;padding-top: 60rpx;}
.top_box .b{display: block;width: 100%;text-align: center;}
.m_box{margin-top: 20rpx;background: #ffffff;padding-bottom: 20rpx;}
.m_box .title{color: #333333;padding-left: 20rpx;height: 80rpx;line-height: 80rpx;border-bottom: solid #f1f1f1 1px;}
.m_box .m{display: flex;font-size: 26rpx;color: #666666;height:100rpx;padding-top: 10rpx;padding-bottom: 10rpx;align-items: center;justify-content: space-between;}
.m_box .m .m_la{margin-left: 20rpx;color: #666666;}
.m_box .m .m_lb{margin-left: 20rpx;color: #666666;width: 200rpx;}
.m_box .m .m_ra{margin-left: 200rpx;color: #666666;}
.m_box .m .m_rb{margin-left: 20rpx;height: 100rpx;width:100rpx;display:inline-block;background: url("data:image/png;base64,") center center no-repeat;background-size: 30rpx 30rpx;
}

.b_box{margin-top: 20rpx;background: #ffffff;padding-bottom: 20rpx;margin-bottom: 20rpx;}
.b_box .t{color: #333333;padding-left: 20rpx;height: 80rpx;line-height: 80rpx;border-bottom: solid #f1f1f1 1px;}

.b_box .m{color: #666666;height: 100rpx;line-height: 80rpx;text-align: center;font-size: 24rpx;}

