/* page/brushInfo/brushInfo.wxss */
.top{width: 100%;height: 100rpx;background: #ffffff;display:flex;}
.top .m{width: 50%;text-align: center;line-height: 100rpx;color:#9c9da3; }
.top .h{border-bottom: solid rgb(79,166,228) 3px;color: rgb(79,166,228);}
.box{}
.box .one{display: none;}
.box .two{display: none;}
.box .h{display: block}
.date_box{background: #ffffff;padding-top: 20rpx;padding-bottom: 20rpx;margin-top: 20rpx;}
.date_box .t{display:flex;}
.date_box .t .t_l{ width: 20%;text-align: center;height: 70rpx;line-height: 70rpx;color: #9c9da3;}
.date_box .t .t_m{ width: 60%;text-align: center;height: 70rpx;line-height: 70rpx;color: #333333;}
.date_box .t .t_r{ width: 20%;text-align: center;height: 70rpx;line-height: 70rpx;color: #9c9da3;}
.date_box .m{}
.date_box .m .m_t{display:flex;}
.date_box .m .m_t .m_t_m{text-align: center;width: 14.28%;font-size: 28rpx;color: #9c9da3;height: 60rpx;line-height: 60rpx;}
.date_box .m .m_m{display:flex;flex-flow: row wrap;}
.date_box .m .m_m .m_m_m{text-align: center;width: 14.28%;font-size: 28rpx;color: #9c9da3;height: 80rpx;line-height: 80rpx;}
.date_box .m .m_b{display:flex;flex-flow: row wrap;font-size: 25rpx;color: #9c9da3;}
.date_box .m .m_b .m_b_t{padding-left: 10rpx;}
.date_box .m .m_b .m_b_a{width: 30rpx;height: 30rpx;border-radius: 15rpx;background: rgb(79,166,228);margin-left: 25rpx;}
.date_box .m .m_b .m_b_b{width: 28rpx;height: 28rpx;border-radius: 16rpx;background: #ffffff;margin-left: 25rpx;border: 1px solid #fa8291;}
.date_box .m .m_b .m_b_c{width: 30rpx;height: 30rpx;border-radius: 15rpx;background: #d7dce0;margin-left: 25rpx;}
.date_box .m .m_b .m_b_d{width: 30rpx;height: 30rpx;margin-left: 25rpx;border-bottom: solid rgb(79,166,228) 2px;}

.type{color: #aaaaaa;}
.type0{background: #ffffff;width: 58rpx;height: 58rpx;border-radius: 30rpx;color: #fa8291;line-height: 58rpx;text-align: center;display: inline-block;border: 1px solid #fa8291;}
.type1{background: rgb(79,166,228);width: 60rpx;height: 60rpx;border-radius: 30rpx;color: #ffffff;line-height: 60rpx;text-align: center;display: inline-block;}
.type2{background: #d7dce0;width: 60rpx;height: 60rpx;border-radius: 30rpx;color: #ffffff;line-height: 60rpx;text-align: center;display: inline-block;}

.day_box{margin-top: 20rpx;background: #ffffff;padding-bottom: 20rpx;margin-bottom: 20rpx;}
.day_box .t{height: 80rpx;line-height: 80rpx;padding-left: 20rpx;color: #333333;}
.day_box .m{display: flex;line-height:100rpx;}
.day_box .m .m_la{margin-left: 20rpx;color: #666666;}
.day_box .m .m_lb{margin-left: 20rpx;color: #666666;}
.day_box .m .m_ra{margin-left: 370rpx;color: #666666;}
.day_box .m .m_rb{margin-left: 20rpx;height: 100rpx;width:100rpx;display:inline-block;background: url("data:image/png;base64,") center center no-repeat;background-size: 30rpx 30rpx;
}
.dateh_box{background: #ffffff;padding-top: 20rpx;padding-bottom: 20rpx;margin-top: 20rpx;}
.dateh_box .t{display:flex;}
.dateh_box .t .t_l{ width: 20%;text-align: center;height: 70rpx;line-height: 70rpx;color: #9c9da3;}
.dateh_box .t .t_m{ width: 60%;text-align: center;height: 70rpx;line-height: 70rpx;color: #333333;}
.dateh_box .t .t_r{ width: 20%;text-align: center;height: 70rpx;line-height: 70rpx;color: #9c9da3;}
.dateh_box .m{display:flex;}
.dateh_box .m .l{width: 80rpx;text-align: right;font-size: 26rpx;color: #aaaaaa;}
.dateh_box .m .l100{height: 150rpx;}
.dateh_box .m .l60{height: 250rpx;}
.dateh_box .m .r .r_m{margin-left: 10rpx;border-bottom: solid #eaeaea 1px;border-left: solid #eaeaea 1px;width: 630rpx;height: 440rpx;display: flex;align-items: baseline;justify-content: space-between;}
.dateh_box .m .r .r_mo{height: 100%;width: 10rpx;background: #eaeaea;margin-left: 10rpx;position: relative;}
.dateh_box .m .r .r_mm{height: 50%;background:rgb(79,166,228);position: absolute;bottom: 0;left: 0;width: 10rpx;border-radius: 5rpx 5rpx 0 0;transition: height 2s; }
.dateh_box .m .r .r_b{display: flex;font-size: 26rpx;color: #aaaaaa;width: 100%;align-items: baseline;justify-content: space-between;}
.dateh_box .m .r .r_bm{text-align: center;}
.month_box{margin-top: 20rpx;background: #ffffff;padding-top: 20rpx;padding-bottom: 20rpx;}
.month_box .t{height: 80rpx;line-height: 80rpx;padding-left: 20rpx;color: #333333;}
.month_box .m{text-align: center;display: flex;flex-flow: row wrap;}
.month_box .m .m_m{width: 50%;}
.month_box .m .m_ma{font-size: 30rpx;color: #333333;}
.month_box .m .m_mb{font-size: 60rpx;display: inline-block;}
.month_box .m .m_mc{font-size: 25rpx;color: #999999;}