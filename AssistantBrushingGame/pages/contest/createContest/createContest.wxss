/* pages/createContest/createContest.wxss */
@import '../../../template/contestTemplate.wxss';
.create-contest-container{
  width: 100%;
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  align-content: center;
  justify-content: center;
}
#create-name-view{
  width: 100%;
  top: 2rpx;
  height: 130rpx;
  display: flex;
  flex-direction: row;
  background-color: #fff;
  align-items: center;
}
#create-name-title{
  margin-left: 30rpx;
  font-size: 28rpx;
  color: #666;
}

.create-input-view{
  width: 570rpx;
  display: flex;
  flex-direction: column;
}
#create-input{
  width: 100%;
  font-size: 28rpx;
  color: #999;
  text-align: right;
}
#create-line{
  left: 144rpx;
  right: 30rpx;
  bottom: 30rpx;
  height: 2rpx;
  background-color: #f5f5f5;
}
#create-total-device-view{
  position: absolute;
  top: 132rpx;
  width: 100%;
  height: 61rpx;
  font-size: 26rpx;
  display: flex;
  align-items: center;
  justify-content: center;
  color: #999;
}

#create-device-list-view{
  position: absolute;
  top: 193rpx;
  width: 100%;  
}