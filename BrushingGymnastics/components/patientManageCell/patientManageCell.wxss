/* components/patientManageCell/patientManageCell.wxss */
.cell-container{
  width: 100%;
  height: 100rpx;
  display: flex;
  flex-direction: column;
}

.item-detail{
  width: 25%;
  align-items: center;
  justify-content: center;
  display: flex;
  font-size: 26rpx;
  color: #4E4E4E;
}

.cell-content{
  width: 100%;
  height: 99rpx;
  display: flex;
  flex-direction: row;
  align-items: center;
}

.item-star {
  width: 30rpx;
  height: 28rpx;
}

.cell-line{
  width: 100%;
  height: 1rpx;
  background-color: #eee;
}

.cell-arrow{
  margin-left: -30rpx;
  width: 44rpx;
  height: 44rpx;
}