/* components/moneyDetailCell/moneyDetailCell.wxss */

.money-detail-cell {
  width: 100%;
  display: flex;
  flex-direction: row;
  justify-content: center;
  align-items: center;
}
.money-left {
  height: 100rpx;
  flex: 1;
  display: flex;
  flex-direction: column;
  justify-content: center;
}
.cell-line{
  width: 100%;
  height: 2rpx;
  background-color: #eee;
}
.money-title{
  font-size: 25rpx;
}
.money-time{
  font-size: 20rpx;
  color: #999;
}
.money-number{
  color: #ea3621;
  font-size: 25rpx;
}