.cell-container{
  width: 100%;
  height: 100rpx;
  background-color: white;
  display: flex;
  flex-direction: column;
  position: relative;
}

.cell-content{
  position: absolute;
  width: 100%;
  top: 0;
  left: 0;
  bottom: 1rpx;
  display: flex;
  flex-direction: row;
  align-items: center;
  justify-content: center;
}

.cell-arrow{
  width: 32rpx;
  height: 32rpx;
  margin-right: 15rpx;
}

.cell-line{
  position: absolute;
  left: 0;
  width: 100%;
  height: 2rpx;
  bottom: 0;
  background-color: #f8f8f8;
}

.cell-detail{
  margin-left: 15rpx;
  color: #999;
  font-size: 28rpx;
}

.cell-title{
  margin-left: 15rpx;
  font-size: 34rpx;
  color: #333;
}

.cell-left {
  display: flex;
  height: 100%;
  flex-direction: row;
  flex: 1;
  align-items: center;
}

.cell-right{
  display: flex;
  flex-direction: row;
  height: 100%;
  align-items: center;
}