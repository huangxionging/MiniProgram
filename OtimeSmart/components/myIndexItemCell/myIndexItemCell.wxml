<!--component/myIndexItemCell/myIndexItemCell.wxml-->
<view class="cell-container" style="height:120rpx;">
  <view class="cell-content" bindtap='didSelectRow'>
    <view class="cell-left">
      <image wx:if="{{data.icon}}" class="cell-icon" src="/resource/{{data.icon}}"></image>
      <view class="cell-title">{{data.title}}</view>
    </view>
    <view class="cell-right" style="flex-direction:row-reverse;">
      <image class="cell-arrow" src="/resource/arrow_right.png"></image>
      <view class="cell-detail">{{data.detail}}</view>
    </view>
  </view>
  <view class="cell-line"/>
</view>
