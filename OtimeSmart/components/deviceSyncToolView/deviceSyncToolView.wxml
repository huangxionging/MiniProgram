<!--components/deviceSyncToolView/deviceSyncToolView.wxml-->
<view class="device-tool-view">
  <!-- <view class="device-mask-view"></view> -->
  <view class="device-container-view">
    <view class="device-content-view">
      <view class="device-line"/>
      <view class="device-object">
        <view class="device-name">{{data.deviceName}}设备</view>
        <view class="device-macAddress">({{data.macAddress}})</view>
      </view>
      <view class="device-state-view">
        <image class="device-state-image" src="cloud://otime-smart-y6yn7.6f74-otime-smart-y6yn7/ble_search.gif" />
        <view class="device-state-tip">{{data.stateText}}</view>
      </view>
    </view>
  </view>
</view>