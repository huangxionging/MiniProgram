.box {
  padding-bottom: 100rpx;
  margin-top: 20rpx;
}

.box .t_box {
  font-size: 36rpx;
  color: #333;
  position: relative;
  padding: 30rpx;
  padding-top: 0;
}

.box .t_box .r_box {
  position: absolute;
  right: 0;
  top: 0;
}

.box .t_box .r_box .btn_01 {
  font-size: 32rpx;
  color: #11a6ea;
  border-radius: 6rpx;
  border: solid #11a6ea 2rpx;
  float: right;
  margin-right: 30rpx;
  padding-left: 20rpx;
  padding-right: 20rpx;
}

.b_btn {
  width: 100%;
  height: 100rpx;
  line-height: 100rpx;
  text-align: center;
  position: fixed;
  bottom: 0;
  left: 0;
  background: #11a6ea;
  color: #fff;
}

.title_01 {
  padding: 30rpx;
  padding-bottom: 0;
}

.checked_box {
  padding: 30rpx;
  padding-top: 5rpx;
  padding-bottom: 5rpx;
}

.checked_box .t {
  font-size: 28rpx;
  color: #333;
  line-height: 60rpx;
}

.checked_box .m {
  display: flex;
  flex-wrap: wrap;
}

.checked_box .m .item {
  width: 25%;
  font-size: 24rpx;
  color: #777;
}

.mark {
  width: 100%;
  height: 100%;
  background: rgba(0, 0, 0, 0.3);
  position: fixed;
  left: 0;
  top: 0;
}

.mark .m_box {
  width: 600rpx;
  height: 700rpx;
  background: #fff;
  border-radius: 8rpx;
  text-align: center;
  position: absolute;
  left: 50%;
  margin-left: -300rpx;
  top: 50%;
  margin-top: -350rpx;
}

.mark .m_box .t {
  height: 100rpx;
  line-height: 100rpx;
  color: #333;
  border-bottom: solid #f1f1f1 1rpx;
}

.mark .m_box .true_btn {
  height: 80rpx;
  line-height: 80rpx;
  background: #11a6ea;
  color: #fff;
  width: 80%;
  margin: 30rpx auto;
  border-radius: 8rpx;
}


.piker-item{
  display: flex;
  align-items: center;
  justify-content: center;
}