/* pages/patientManage/patientManage/patientManage.wxss */
page {
  width: 100%;
  height: 100%;
}
.patient-container{
  width: 100%;
  height: 100%;
  display: flex;
  flex-direction: column;
}

.patient-item-header{
  position: fixed;
  top: 108rpx;
  left: 0;
  width: 100%;
  height: 80rpx;
  display: flex;
  flex-direction: row;
  align-items: center;
  color: #888;
  font-size: 26rpx;
  background-color: white;
}

.patient-item-header-line{
  width: 100%;
  height: 1rpx;
  position: fixed;
  top: 189rpx;
  left: 0;
  background-color: #eee;
}
.item-serial {
  width: 25%;
  height: 100%;
  align-items: center;
  display: flex;
  justify-content: center;
}

.patient-table-view{
  position: relative;
  margin-top: 190rpx;
  width: 100%;
}

.head-view{
  position: fixed;
  top: 0;
  background-color: #00a0e9;
  width: 100%;
  height: 108rpx;
}