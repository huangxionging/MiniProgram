/**app.wxss**/
.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 200rpx 0;
  box-sizing: border-box;
} 
.main-button{
  width: 400rpx;
  background-color: #00a0e9;
  color: #fff;
  font-size: 30rpx;
}

.hover-main-button{
  background-color: #02557c;
  color: #a6c4d2;
}

.navigation-bar-line{
  width: 100%;
  height: 2rpx;
  margin-top: 0rpx;
  background-color: #f5f5f5;
}